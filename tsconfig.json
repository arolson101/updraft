{
    "version": "1.6",
    "compilerOptions": {
        "target": "es6",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": false,
        "noEmit": true
    },
    "filesGlob": [
        "./src/**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/index.ts",
        "./src/Column.ts",
        "./test/test.ts"
    ]
}
