{
		"version": "1.6",
		"compilerOptions": {
				"target": "es5",
				"module": "commonjs",
				"declaration": false,
				"noImplicitAny": true,
				"removeComments": false,
				"noLib": false,
				"preserveConstEnums": true,
				"suppressImplicitAnyIndexErrors": true,
				"sourceMap": false,
				"inlineSourceMap": false,
				"noEmit": false
		},
		"filesGlob": [
				"./src/**/*.ts",
				"./test/**/*.ts",
				"!./node_modules/**/*.ts"
		],
		"files": [
				"./src/Column.ts",
				"./src/Mutate.ts",
				"./src/Query.ts",
				"./src/Store.ts",
				"./src/Table.ts",
				"./src/WebsqlWrapper.ts",
				"./src/index.ts",
				"./src/websql.d.ts",
				"./test/mutate.test.ts",
				"./test/table.test.ts"
		]
}
